{
	"name": "@blaxel/core",
	"version": "0.2.4-dev4",
	"description": "Blaxel Core SDK for TypeScript",
	"license": "MIT",
	"author": "Blaxel, INC (https://blaxel.ai)",
	"homepage": "https://blaxel.ai",
	"scripts": {
		"lint": "eslint src/",
		"dev": "tsc --watch",
		"build": "tsc",
		"test": "vitest --run",
		"test:watch": "vitest --watch"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blaxel/toolkit/sdk-ts"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"blaxel",
		"agent",
		"mcp"
	],
	"main": "dist/index.js",
	"module": "./dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			},
			"require": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*"
			]
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@hey-api/client-fetch": "^0.10.0",
		"@modelcontextprotocol/sdk": "^1.11.0",
		"dotenv": "^16.5.0",
		"jwt-decode": "^4.0.0",
		"toml": "^3.0.0",
		"uuid": "^11.1.0",
		"ws": "^8.18.2",
		"yaml": "^2.7.1",
		"zod": "^3.24.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.26.0",
		"@testing-library/dom": "^9.3.0",
		"@types/ws": "^8.18.1",
		"jsdom": "^26.1.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.31.1",
		"vite": "^5.2.0",
		"vitest": "^1.5.0"
	}
}
