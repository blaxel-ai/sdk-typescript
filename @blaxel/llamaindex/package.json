{
	"name": "@blaxel/llamaindex",
	"version": "0.2.4-dev4",
	"description": "Blaxel SDK for TypeScript",
	"license": "MIT",
	"author": "Blaxel, INC (https://blaxel.ai)",
	"homepage": "https://blaxel.ai",
	"scripts": {
		"lint": "eslint src/",
		"dev": "tsc --watch",
		"build": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blaxel-ai/sdk-typescript"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"blaxel",
		"agent",
		"mcp"
	],
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			},
			"require": {
				"types": "./dist/*.d.ts",
				"default": "./dist/*.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/index.d.ts"
			],
			"*": [
				"./dist/*"
			]
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@blaxel/core": "workspace:*",
		"@llamaindex/anthropic": "^0.3.15",
		"@llamaindex/core": "0.6.13",
		"@llamaindex/google": "0.3.12",
		"@llamaindex/mistral": "^0.1.13",
		"@llamaindex/openai": "^0.4.7",
		"@opentelemetry/instrumentation": "^0.203.0",
		"@traceloop/instrumentation-llamaindex": "^0.14.0",
		"llamaindex": "^0.11.13"
	},
	"devDependencies": {
		"@eslint/js": "^9.30.1",
		"eslint": "^9.30.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.36.0"
	}
}
