{
	"name": "@blaxel/langgraph",
	"version": "0.2.4-dev4",
	"description": "Blaxel SDK for TypeScript",
	"license": "MIT",
	"author": "Blaxel, INC (https://blaxel.ai)",
	"homepage": "https://blaxel.ai",
	"scripts": {
		"lint": "eslint src/",
		"dev": "tsc --watch",
		"build": "npm run build:cjs && npm run build:esm && npm run build:fix-esm && npm run build:esm-package",
		"build:cjs": "NODE_OPTIONS='--max-old-space-size=8192' tsc -p tsconfig.cjs.json",
		"build:esm": "NODE_OPTIONS='--max-old-space-size=8192' tsc -p tsconfig.esm.json",
		"build:fix-esm": "node fix-esm-imports.js",
		"build:esm-package": "echo '{\"type\":\"module\"}' > dist/esm/package.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blaxel-ai/sdk-typescript"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"blaxel",
		"agent",
		"mcp"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/types/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/cjs/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			".": [
				"./dist/cjs/types/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@blaxel/core": "workspace:*",
		"@google/generative-ai": "^0.24.1",
		"@langchain/anthropic": "^0.3.20",
		"@langchain/cohere": "^0.3.3",
		"@langchain/core": "^0.3.51",
		"@langchain/deepseek": "^0.0.1",
		"@langchain/openai": "^0.5.10",
		"@opentelemetry/instrumentation": "^0.200.0",
		"@traceloop/instrumentation-langchain": "^0.13.0",
		"cohere-ai": "^7.17.1",
		"langchain": "^0.3.24",
		"zod": "^3.24.3",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.26.0",
		"eslint": "^9.27.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.31.1"
	}
}
