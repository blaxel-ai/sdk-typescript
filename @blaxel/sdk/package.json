{
	"name": "@blaxel/sdk",
	"version": "0.1.16-preview.2",
	"description": "Blaxel SDK for TypeScript",
	"license": "MIT",
	"author": "Blaxel, INC (https://blaxel.ai)",
	"homepage": "https://blaxel.ai",
	"bugs": "https://github.com/blaxel/toolkit/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/blaxel/toolkit/sdk-ts"
	},
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"blaxel",
		"agent",
		"mcp"
	],
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/esm/*.d.ts",
				"default": "./dist/esm/*.js"
			},
			"require": {
				"types": "./dist/cjs/*.d.ts",
				"default": "./dist/cjs/*.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/esm/*"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "npm run build:esm && npm run build:cjs",
		"build:watch": "tsc -p tsconfig.prod.json --watch",
		"build:esm": "tsc -p tsconfig.prod.json",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"prepack": "npm run build:esm && npm run build:cjs",
		"lint": "eslint src/",
		"test": "jest",
		"start": "npm run server",
		"server": "tsx watch --clear-screen=false src/cli.ts server",
		"client": "tsx src/cli.ts client",
		"docs": "typedoc"
	},
	"dependencies": {
		"@hey-api/client-fetch": "^0.5.7",
		"@modelcontextprotocol/sdk": "^1.9.0",
		"jwt-decode": "^4.0.0",
		"toml": "^3.0.0",
		"uuid": "^11.1.0",
		"ws": "^8.18.1",
		"yaml": "^2.7.1",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.24.0",
		"@jest/globals": "^29.7.0",
		"@tsconfig/node20": "^20.1.5",
		"@types/jest": "^29.5.14",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^22.14.0",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^8.29.1",
		"@typescript-eslint/parser": "^8.29.1",
		"dotenv": "^16.5.0",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"jest": "^29.7.0",
		"prebuilt": "link:@langchain/langgraph/prebuilt",
		"ts-jest": "^29.3.1",
		"tsx": "^4.19.3",
		"typedoc": "^0.27.9",
		"typedoc-plugin-markdown": "^4.6.2",
		"typedoc-theme-hierarchy": "^5.0.5",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1"
	},
	"jest": {
		"preset": "ts-jest/presets/default-esm",
		"moduleNameMapper": {
			"^(\\.{1,2}/.*)\\.js$": "$1"
		},
		"extensionsToTreatAsEsm": [
			".ts"
		],
		"transform": {
			"^.+\\.tsx?$": [
				"ts-jest",
				{
					"useESM": true,
					"tsconfig": "tsconfig.json"
				}
			]
		},
		"testEnvironment": "node"
	}
}
