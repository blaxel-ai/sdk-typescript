{
  "name": "sdk-ts",
  "version": "0.2.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "bun --filter '*' lint && bun run lint:tests",
    "lint:tests": "tsc -p tests/tsconfig.json && eslint tests/",
    "build": "bun --filter '*' build",
    "set-version": "npm version --workspaces --no-workspaces-update",
    "publish-dev": "bun run build && bun --filter '*' publish --tag dev --access public",
    "publish-preview": "bun run build && bun --filter '*' publish --tag preview --access public",
    "publish-prod": "bun run build && bun --filter '*' publish --tag latest --access public",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:integration:sandbox": "vitest run tests/integration/sandbox",
    "test:integration:models": "vitest run tests/integration/models",
    "test:integration:tools": "vitest run tests/integration/tools",
    "test:integration:jobs": "vitest run tests/integration/jobs",
    "test:e2e": "vitest run tests/e2e --config vitest.e2e.config.ts",
    "test:runtime-environments": "./tests/runtime-environments/test-all.sh",
    "bench": "vitest bench --run",
    "bench:sandbox": "vitest run bench tests/benchmarks/sandbox"
  },
  "dependencies": {
    "@blaxel/core": "workspace:*",
    "@blaxel/langgraph": "workspace:*",
    "@blaxel/llamaindex": "workspace:*",
    "@blaxel/mastra": "workspace:*",
    "@blaxel/telemetry": "workspace:*",
    "@blaxel/vercel": "workspace:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "bun@1.3.4",
  "devDependencies": {
    "@langchain/core": "^1.0.6",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/openai": "^1.1.2",
    "@mastra/core": "^0.24.5",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@opentelemetry/api-logs": "^0.200.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.200.0",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.15.3",
    "adm-zip": "^0.5.16",
    "ai": "^5.0.82",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fastify": "^5.3.2",
    "langchain": "^1.0.6",
    "llamaindex": "^0.12.0",
    "@eslint/js": "^9.26.0",
    "typescript-eslint": "^8.31.1",
    "uuid": "^11.1.0",
    "vitest": "^2.1.0",
    "zod": "^3.24.3"
  },
  "workspaces": [
    "@blaxel/core",
    "@blaxel/telemetry",
    "@blaxel/langgraph",
    "@blaxel/llamaindex",
    "@blaxel/vercel",
    "@blaxel/mastra"
  ]
}
